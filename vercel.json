{
    "version": 2,
    "builds": [
        {
            "src": "*.go",
            "use": "@vercel/go"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "GO_VERSION": "1.21"
    },
    "build": {
        "env": {
            "GO111MODULE": "on"
        }
    }
}